<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Google Backup Transport Service-->
    <bool name="def_backup_enabled">true</bool>
    <string name="def_backup_transport">com.google.android.gms/.backup.BackupTransportService</string>


    <!-- Non Market Apps -->
    <bool name="def_install_non_market_apps">true</bool>

    <!-- Disable "vibrate on touch" -->
    <bool name="def_haptic_feedback">false</bool>
    
    <!-- Animation scales (Development Settings) -->
    <fraction name="def_window_animation_scale">75%</fraction>
    <fraction name="def_window_transition_scale">75%</fraction>
</resources> 
